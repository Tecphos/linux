/dts-v1/;

#include <dt-bindings/interrupt-controller/irq.h>
#include <dt-bindings/input/input.h>
#include "imx6dl.dtsi"
#include "imx6qdl-wandboard.dtsi"

/ {
	model = "Wandboard i.MX6 Dual Lite Board";
	compatible = "wand,imx6dl-wandboard", "fsl,imx6dl";

	uio_fpga {
		compatible = "generic-uio";
		interrupts = < 14 IRQ_TYPE_EDGE_RISING >;
		interrupt-parent = <&gpio6>;
		pinctrl-names = "default";
		pinctrl-0 = <&pinctrl_fpga>;
	};

	bcmdhd_wlan_0: bcmdhd_wlan@0 {
		compatible = "android,bcmdhd_wlan";
		pinctrl-0 = <&pinctrl_wifi_ctrl>;
		wlreg_on-supply = <&wlreg_on>;
	};

	regulators {
		compatible = "simple-bus";
		#address-cells = <1>;
		#size-cells = <0>;

		wlreg_on: fixedregulator@100 {
			compatible = "regulator-fixed";
			regulator-min-microvolt = <5000000>;
			regulator-max-microvolt = <5000000>;
			regulator-name = "wlreg_on";
			gpio = <&gpio6 0 GPIO_ACTIVE_HIGH>;
			enable-active-high;
		};
	};

	gpio-edm {
		compatible = "gpio-edm";

		wl_on {
			label = "wl_on";
			gpios = <&gpio5 31 1>;
			dir   = "out";
		};

		wl_reg {
			label = "wl_reg";
			gpios = <&gpio1 26 1>;
			dir   = "out";
		};

		bluetooth-on {
			label = "bluetooth-on";
			gpios = <&gpio5 21 1>;
			dir   = "out";
		};

		bluetooth-wake {
			label = "bluetooth-wake";
			gpios = <&gpio5 30 1>;
			dir   = "in";
		};

		bluetooth-host-wake {
			label = "bluetooth-host-wake";
			gpios = <&gpio5 20 1>;
			dir   = "in";
		};
	};

};

&ldb {
	lvds-channel@0 {
		crtc = "ipu1-di1";
	};
};

&mxcfb1 {
	status = "okay";
};

&mxcfb2 {
	status = "okay";
};

#if 0
&mxcfb3 {
	status = "okay";
};

&mxcfb4 {
	status = "okay";
};
#endif

&epdc {
	status = "disabled";
};

&gpmi {
        pinctrl-names = "default";
        pinctrl-0 = <&pinctrl_gpmi_nand_2>;
        status = "okay";
};

&ecspi1 {
        fsl,spi-num-chipselects = <1>;
        cs-gpios = <&gpio2 30 0>;
        pinctrl-names = "default";
        pinctrl-0 = <&pinctrl_ecspi1>;
        status = "okay";

        /delete-node/ ads7846@0;

        spidev@0 {
                compatible = "spidev";
                reg = <0>;
                #address-cells = <1>;
                #size-cells = <0>;
                spi-max-frequency = <20000000>;
        };

};

&iomuxc {
	pinctrl-names = "default";

	imx6qdl-wandboard {

                pinctrl_ecspi1: ecspi1grp {
                        fsl,pins = <
                                MX6QDL_PAD_EIM_EB2__GPIO2_IO30 0x80000000
                        >;
                };

		pinctrl_wifi_ctrl: wifi_ctrlgrp {
			fsl,pins = <
				MX6QDL_PAD_CSI0_DAT14__GPIO6_IO00 0x000b0			/* wifi-reset */
				MX6QDL_PAD_CSI0_DAT13__GPIO5_IO31 0x000b0			/* wifi-on */
				MX6QDL_PAD_ENET_RXD1__GPIO1_IO26  0x000b0			/* wifi-reg-on */
				MX6QDL_PAD_ENET_TXD1__GPIO1_IO29  0x80000000		/* wifi-host-wake */

			>;
		};

		pinctrl_bt_ctrl: bt_ctrlgrp {
			fsl,pins = <
				MX6QDL_PAD_CSI0_VSYNC__GPIO5_IO21	0x000b0		/* bluetooth-reg-on */
				MX6QDL_PAD_CSI0_DAT12__GPIO5_IO30	0x80000000	/* bluetooth-wake */
				MX6QDL_PAD_CSI0_DATA_EN__GPIO5_IO20	0x80000000	/* bluetooth-host-wake */
			>;
		};
	};
	gpmni-nand {
                pinctrl_gpmi_nand_2: gpmi-nand-2 {
                        fsl,pins = <
                                MX6QDL_PAD_NANDF_CLE__NAND_CLE     0xb0b1
                                MX6QDL_PAD_NANDF_ALE__NAND_ALE     0xb0b1
                                MX6QDL_PAD_NANDF_WP_B__NAND_WP_B   0xb0b1
                                MX6QDL_PAD_NANDF_RB0__NAND_READY_B 0xb000
                                MX6QDL_PAD_NANDF_CS0__NAND_CE0_B   0xb0b1
                                MX6QDL_PAD_SD4_CMD__NAND_RE_B      0xb0b1
                                MX6QDL_PAD_SD4_CLK__NAND_WE_B      0xb0b1
                                MX6QDL_PAD_NANDF_D0__NAND_DATA00   0xb0b1
                                MX6QDL_PAD_NANDF_D1__NAND_DATA01   0xb0b1
                                MX6QDL_PAD_NANDF_D2__NAND_DATA02   0xb0b1
                                MX6QDL_PAD_NANDF_D3__NAND_DATA03   0xb0b1
                                MX6QDL_PAD_NANDF_D4__NAND_DATA04   0xb0b1
                                MX6QDL_PAD_NANDF_D5__NAND_DATA05   0xb0b1
                                MX6QDL_PAD_NANDF_D6__NAND_DATA06   0xb0b1
                                MX6QDL_PAD_NANDF_D7__NAND_DATA07   0xb0b1
                                MX6QDL_PAD_SD4_DAT0__NAND_DQS      0x00b1
                        >;
                };
        };
	fpga {
                pinctrl_fpga: fpga_int {
                        fsl,pins = <
                                MX6QDL_PAD_NANDF_CS1__GPIO6_IO14   0x1b0b0
                        >;
                };
        };
};
